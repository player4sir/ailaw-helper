{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server/index.mjs",
    "dev:server": "node server/index.mjs",
    "dev:all": "concurrently \"npm:dev:server\" \"npm:dev\"",
    "build": "vite build",
    "build:cf": "npm run build && npm run copy-functions",
    "copy-functions": "cp -r functions dist/ || xcopy functions dist\\functions /E /I",
    "lint": "eslint .",
    "preview": "vite preview",
    "check-env": "node check-env.mjs",
    "deploy": "npm run build:cf"
  },
  "dependencies": {
    "docx": "^8.5.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.1",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/express": "^4.17.21",
    "@types/node": "^22.4.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
